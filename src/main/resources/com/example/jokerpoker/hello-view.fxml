<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="mainwin" prefHeight="455.0" prefWidth="354.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jokerpoker.HelloController">
    <children>
        <VBox prefHeight="453.0" prefWidth="354.0">
            <children>
                <HBox alignment="TOP_RIGHT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="21.0" prefWidth="354.0">
                    <children>
                        <Button fx:id="btnMin" mnemonicParsing="false" prefHeight="10.0" prefWidth="20.0">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </HBox.margin>
                            <font>
                                <Font size="10.0" />
                            </font></Button>
                        <Button fx:id="btnClose" mnemonicParsing="false" prefHeight="10.0" prefWidth="22.0">
                            <HBox.margin>
                                <Insets right="10.0" top="10.0" />
                            </HBox.margin>
                            <font>
                                <Font size="10.0" />
                            </font>
                        </Button>
                    </children>
                </HBox>
                <Label fx:id="labelTitle" alignment="CENTER" prefHeight="89.0" prefWidth="354.0" text="LOGIN">
                    <font>
                        <Font name="System Bold" size="40.0" />
                    </font></Label>
                <TextField fx:id="textUsername" alignment="CENTER" prefHeight="49.0" prefWidth="286.0" promptText="Username">
                    <VBox.margin>
                        <Insets bottom="15.0" left="60.0" right="60.0" top="15.0" />
                    </VBox.margin></TextField>
                <TextField fx:id="textPassword" alignment="CENTER" prefHeight="49.0" prefWidth="234.0" promptText="Password">
                    <VBox.margin>
                        <Insets bottom="15.0" left="60.0" right="60.0" top="15.0" />
                    </VBox.margin></TextField>
                <Button fx:id="btnLogin" alignment="CENTER" mnemonicParsing="false" prefHeight="64.0" prefWidth="154.0" text="SUBMIT">
                    <font>
                        <Font size="19.0" />
                    </font>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <VBox.margin>
                        <Insets bottom="10.0" left="100.0" right="100.0" top="10.0" />
                    </VBox.margin>
                </Button>
                <HBox alignment="CENTER" prefHeight="52.0" prefWidth="354.0">
                    <children>
                        <Label fx:id="labelText" text="No Account ? -&gt;" textFill="#ced1d4" />
                        <Button fx:id="btnEnroll" mnemonicParsing="false" text="Enroll" textFill="#ced1d4" />
                    </children>
                </HBox>
            </children></VBox>
    </children>
    <stylesheets>
        <URL value="@new.css" />
    </stylesheets>

</AnchorPane>
